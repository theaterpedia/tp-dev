{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".output/**",
        ".histoire/**"
      ]
    },
    "generate" : {
      "dependsOn": ["^generate"],
      "outputs": [
        "dist/**",
        ".output/**"
      ]
    },
    "release" : {
      "dependsOn": ["^release"],
      "outputMode": "new-only"
    },    
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "codegen": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },   
    "test:turbo": {
      "outputs": []
    },
    "type-check": {}
  }
}
